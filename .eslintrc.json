{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "react/no-unescaped-entities": "off",
    "no-unused-vars": [
      "warn",
      {
        "varsIgnorePattern": "^(t|translations|index|_)$",
        "argsIgnorePattern": "^_"
      }
    ],
    "@next/next/no-img-element": "off"
  }
}
